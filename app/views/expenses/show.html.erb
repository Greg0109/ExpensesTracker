<div class="w-100 d-flex justify-content-center text-center">
    <div class="column">
        <h1>Total spent</h1>
        <p><%= @total %></p>
        <h1 class="col-12">All Expenses</h1>
        <% @expenses.each do |exp| %>
            <p><%= exp.name %></p>
            <p><%= exp.amount %></p>
            <p><%= exp.created_at %></p>
        <% end %>
    </div>
</div>